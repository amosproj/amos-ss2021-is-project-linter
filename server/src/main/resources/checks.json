{
  "linter": {
    "settings": {
      "scheduler": "0 0 * * *"
    },
    "checks": {
      "file_checks": [
        {
          "name": "checkReadmeExistence",
          "enabled": true,
          "severity_level": 5
        },
        {
          "name": "checkContributingExistence",
          "enabled": true
        },
        {
          "name": "checkMaintainersExistence",
          "enabled": true
        }
      ],
      "settings_checks": [
        {
          "name": "hasMergeRequestEnabled",
          "enabled": true
        },
        {
          "name": "hasRequestAccessEnabled",
          "enabled": false
        },
        {
          "name": "hasIssuesEnabled",
          "enabled": true
        },
        {
          "name": "isPublic",
          "enabled": true
        },
        {
          "name": "gitlabPagesEnabled",
          "enabled": true
        }
      ],
      "roles_checks": [
        {
          "name": "guestRoleEnabled",
          "enabled": true
        },
        {
          "name": "developerRoleEnabled",
          "enabled": true
        }
      ]
    }
  }
}