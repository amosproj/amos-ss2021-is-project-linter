{
  "settings": {
    "scheduler": "0 0 * * *"
  },
  "checks": {
    "file_checks": [
      {
        "name": "checkReadmeExistence",
        "enabled": true,
        "severity": "HIGH"
      },
      {
        "name": "checkContributingExistence",
        "enabled": true,
        "severity": "MEDIUM"
      },
      {
        "name": "checkMaintainersExistence",
        "enabled": true,
        "severity": "MEDIUM"
      }
    ],
    "settings_checks": [
      {
        "name": "hasMergeRequestEnabled",
        "enabled": true,
        "severity": "LOW"
      },
      {
        "name": "hasIssuesEnabled",
        "enabled": true,
        "severity": "MEDIUM"
      },
      {
        "name": "isPublic",
        "enabled": true,
        "severity": "HIGH"
      },
      {
        "name": "gitlabWikiEnabled",
        "enabled": true,
        "severity": "MEDIUM"
      },
      {
        "name": "hasForkingEnabled",
        "enabled": true,
        "severity": "MEDIUM"
      }
    ],
    "roles_checks": [
      {
        "name": "guestRoleEnabled",
        "enabled": true,
        "severity": "LOW"
      },
      {
        "name": "developerRoleEnabled",
        "enabled": true,
        "severity": "LOW"
      }
    ]
  }
